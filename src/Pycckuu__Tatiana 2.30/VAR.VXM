<!-- ************************************************************** -->
<!-- Do not attempt to speak empty names -->
<!-- ************************************************************** -->
	<variable>$USR_COMMA_K_NEXT_ROAD
		<condition>m/@GRM_NEXT_ROAD/@GRM_EMPTY/</condition>
			<entry> </entry>
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>, к $USR_NEXT_ROAD_C3</entry>
	</variable>
	<variable>$USR_COMMA_NA_NEXT_ROAD
		<condition>m/@GRM_NEXT_ROAD/@GRM_EMPTY/</condition>
			<entry> </entry>
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>, на $USR_NEXT_ROAD_C4</entry>
	</variable>
	<variable>$USR_PO_CUR_ROAD
		<condition>m/@GRM_CUR_ROAD/@GRM_EMPTY/</condition>
			<entry>по текущей дороге</entry>
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>по $USR_CUR_ROAD_C3</entry>
	</variable>
	<variable>$USR_DESTINATION
		<condition>m/@GRM_DESTINATION/@GRM_EMPTY/</condition>
			<entry> </entry>
