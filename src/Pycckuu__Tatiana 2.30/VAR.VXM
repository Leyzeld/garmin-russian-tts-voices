	</variable>
	<variable>$USR_K_NEXT_ROAD
		<condition>m/@GRM_NEXT_ROAD/@GRM_EMPTY/</condition>
			<entry> </entry>
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>к $USR_NEXT_ROAD_C3</entry>
	</variable>
	<variable>$USR_NA_NEXT_ROAD
		<condition>m/@GRM_NEXT_ROAD/@GRM_EMPTY/</condition>
			<entry> </entry>
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>на $USR_NEXT_ROAD_C4</entry>
	</variable>
	<variable>$USR_PO_NEXT_ROAD
		<condition>m/@GRM_NEXT_ROAD/@GRM_EMPTY/</condition>
			<entry> </entry>
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>по $USR_NEXT_ROAD_C3</entry>
	</variable>
	<variable>$USR_AND_GO_NA_NEXT_ROAD
