			<entry> </entry>
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>и продолжите движение по $USR_NEXT_ROAD_C3</entry>
	</variable>
	<variable>$USR_LEADING_TO_NEXT_ROAD
		<condition>m/@GRM_NEXT_ROAD/@GRM_EMPTY/</condition>
			<entry> </entry>
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>, ведущий к $USR_NEXT_ROAD_C3</entry>
	</variable>
<!-- ************************************************************** -->
<!-- Temporary variables -->
<!-- These have temporary definitions to allow us to build an experimental -->
<!-- release. They need to be changed once we figure out how to handle them. -->
<!-- Extra line to circumvent 1000-byte boundary issue -->
<!-- ************************************************************** -->
	<variable>$USR_NEXT_ROAD_C3
		<condition>m/@GRM_DEFAULT/@GRM_DEFAULT/</condition>
			<entry>@GRM_NEXT_ROAD</entry>
	</variable>
	<variable>$USR_NEXT_ROAD_C4
